#include<stdio.h>

int main()
{
    int arr[5]={2,4,7,8,1};
    int max=arr[0];
    int smax=arr[0];
    for(int i=1;i<5;i++){
        if(max<arr[i])
        max=arr[i];
    }
   for(int i=1;i<5;i++){
        if(smax<arr[i] && arr[i]!=max)
        smax=arr[i];
    }
    printf("%d",smax);
    return 0;
}
